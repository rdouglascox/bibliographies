@article{Larmore:2013aa,
	address = {Leiden},
	annote = {ark:/67375/JKT-1RR85472-R},
	author = {Larmore, Charles},
	date-added = {2023-06-13 12:20:01 +1000},
	date-modified = {2023-06-13 12:20:01 +1000},
	doi = {10.1163/174552412X628896},
	id = {cdi{\_}proquest{\_}miscellaneous{\_}1429623425},
	isbn = {1740-4681},
	journal = {Journal of moral philosophy},
	journal1 = {JMP},
	keywords = {Coercion ; Ethics ; justice ; legitimacy ; Political Philosophy ; realism ; reasonable disagreement ; reasons ; Truth},
	n2 = {What is political philosophy's relation to moral philosophy? Does it simply form part of moral philosophy, focusing on the proper application of certain moral truths to political reality? Or must it instead form a more autonomous discipline, drawing its bearings from the specifically political problem of determining the bounds of legitimate coercion? In this essay I work out an answer to these questions by examining both some of the classical views on the nature of political philosophy and, more particularly, some recently published writings by Bernard Williams and G.A. Cohen.},
