	keywords = {Alienation ; Authority ; Cooperation ; Despotism ; Ethics ; International cooperation ; Intimidation ; Legitimacy ; Liberalism ; Philosophers ; Political violence ; Power ; Social scientists ; Stability ; Tyranny ; Weber, Max (1864-1920)},
	language = {eng},
	number = {2},
	pages = {295-324},
	publisher = {De Gruyter},
	title = {Legitimacy without Liberalism: A Defense of Max Weber's Standard of Political Legitimacy},
