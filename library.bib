	journal = {Critical review of international social and political philosophy},
	keywords = {Attitudes ; Authority ; Citizens ; Consent ; Constitutions ; Criteria ; Individual and society ; Informed Consent ; Justice ; Legitimacy ; Liberalism ; Libertarians ; Philosophy ; Political authority ; Political Factors ; Politics},
	language = {eng},
	number = {2},
	pages = {129-148},
	publisher = {Routledge},
	title = {Political legitimacy, justice and consent},
	volume = {15},
	year = {2012},
